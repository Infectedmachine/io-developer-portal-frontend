    "italia-tslint-rules": "^1.1.0",
    "parcel-bundler": "^1.10.2",
    "prettier": "^1.17.0",
    "request": "^2.88.0",
    "shx": "^0.3.2",
    "tslint": "^5.16.0",
    "typescript": "^3.4.2"
  }
